<%- @title = "Sass Extension Authors" %>
<%- @h1 = "Extension Authors" %>
<%- @selected = :authors %>

<h2><%= @h1 || @title %></h2>
<div class="records_count"><%= record_count_for @authors, User, :extension_authors_count %></div>

<%= pagination_for @authors %>

<table class="index" id="authors" border="0" cellpadding="0" cellspacing="0">
  <tbody>
    <%= render :partial => 'author', :collection => @authors %>
  </tbody>
</table>

<%= pagination_for @authors %>
